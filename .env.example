# Environment Variables Example
# Copy this file to .env.local and fill in your values

# Booking/Registration URL (Typeform)
NEXT_PUBLIC_BOOKING_URL=https://harbour-space.typeform.com/to/gxFFkidp?typeform-source=join.leaguesofcode.com

# Social Media URLs (optional)
NEXT_PUBLIC_INSTAGRAM_URL=
NEXT_PUBLIC_FACEBOOK_URL=

# Contact Email (optional - defaults to hello@leaguesofcode.com)
NEXT_PUBLIC_EMAIL=hello@leaguesofcode.com

# Public site URL for metadata canonical/base
NEXT_PUBLIC_SITE_URL=https://leaguesofcode.com

# Google Analytics ID (set to disable/enable GA)
NEXT_PUBLIC_GA_ID=G-EDGYEM2760
